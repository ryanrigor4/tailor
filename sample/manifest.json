{
    "manifest_version": 3,
    "name": "PDF Downloader",
    "version": "1.0",
    "permissions": [
      "downloads",
      "storage",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }